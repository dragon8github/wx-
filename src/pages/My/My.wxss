.banner {
  width: 100%;
  height: 328rpx;
  position: relative; }

.banner__bg {
  display: inline; }

.userinfo {
  position: absolute;
  bottom: 30rpx;
  left: 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  color: #fff; }

.userinfoleft__head {
  width: 130rpx;
  height: 130rpx;
  border-radius: 100%;
  border: 10rpx solid #b6cced;
  margin-right: 45rpx; }

.userinforight {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  flex-direction: column;
  font-size: 32rpx;
  height: 90rpx; }

.cell {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  background: #fff;
  min-height: 88rpx;
  overflow: hidden; }

.cell_image {
  margin: 0 24rpx 0 32rpx; }

.cell__icon {
  width: 44rpx;
  height: 44rpx; }

.cell__warp {
  line-height: 88rpx;
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1; }

.u-border-bottom {
  border-bottom: 1rpx solid #e5e5e5; }

.cell__title {
  font-size: 28rpx;
  color: #222222; }
